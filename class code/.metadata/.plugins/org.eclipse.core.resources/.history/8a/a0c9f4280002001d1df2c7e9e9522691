package lesson9.exercise_1;


import java.util.stream.Collectors;
import java.util.stream.Stream;

public class NaturalNumber {
	
	public static void main(String[] args) {
		Stream<Integer> s = Stream.iterate(1, x -> x + 2);
		s.limit(16).collect(Collectors.toList()).forEach(System.out::println);
		
		Stream<Integer> s1 = Stream.iterate(1, x -> x + 2);
		s1.skip(4).limit(4).collect(Collectors.toList()).forEach(System.out::print);
	}

}
